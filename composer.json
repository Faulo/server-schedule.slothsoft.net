{
	"name" : "slothsoft/schedule.slothsoft.net",
	"type" : "project",
	"description" : "schedule.slothsoft.net",
	"license" : "WTFPL",
	"keywords" : [
		"slothsoft",
		"farah-module",
		"farah-server"
	],
	"autoload" : {
		"files" : [
			"config.php",
			"scripts/bootstrap.php"
		],
		"psr-4" : {
			"Slothsoft\\Server\\Schedule\\" : "src"
		}
	},
	"config" : {
		"platform" : {
			"php" : "7.4"
		}
	},
	"require" : {
		"php" : "7.4 - 8.0",
		"slothsoft/core" : "^1.6",
		"slothsoft/farah" : "^1.7",
		"google/apiclient" : "^2.0",
		"chillerlan/php-qrcode" : "^3.4.1"
	},
	"require-dev" : {
		"phpunit/phpunit" : "8.5.*"
	},
	"autoload-dev" : {
		"psr-4" : {
			"Slothsoft\\Server\\Schedule\\" : "tests"
		}
	},
	"scripts" : {
		"post-autoload-dump" : "Slothsoft\\Core\\ServerEnvironment::cleanCacheDirectory",
		"farah-asset" : "@php vendor/slothsoft/farah/scripts/farah-asset.php",
		"farah-page" : "@php vendor/slothsoft/farah/scripts/farah-page.php"
	},
	"homepage" : "http://schedule.slothsoft.net",
	"authors" : [{
			"name" : "Daniel Schulz",
			"email" : "info.slothsoft@gmail.com"
		}
	]
}